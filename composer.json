{
  "name"        : "src-run/augustus-inflector-bundle",
  "license"     : "MIT",
  "type"        : "library",
  "homepage"    : "https://src.run/augustus-inflector-bundle",
  "description" : "tiny bundle to provide service and twig extension for symfony's inflector library (handles de-pluralization)",
  "support" : {
    "email"  : "rmf@src.run",
    "source" : "https://src.run/augustus-inflector-bundle/git",
    "issues" : "https://src.run/augustus-inflector-bundle/issues"
  },
  "keywords" : [
    "augustus-inflector-bundle", "inflector", "augustus", "bundle", "rmf", "src-run", "sr"
  ],
  "authors" : [
    {
      "name"     : "Rob Frawley 2nd",
      "email"    : "rmf@src.run",
      "homepage" : "https://src.run/rmf",
      "role"     : "Project Lead"
    }
  ],
  "require" : {
    "php"                          : "~7.0",
    "src-run/wonka-bundle"         : "~0.7",
    "symfony/inflector"            : "~3.0",
    "symfony/config"               : "~3.0",
    "symfony/dependency-injection" : "~3.0",
    "symfony/http-foundation"      : "~3.0",
    "symfony/twig-bundle"          : "~3.0",
    "twig/twig"                    : "~2.0"
  },
  "require-dev" : {
    "symfony/var-dumper"       : "~3.0",
    "symfony/symfony"          : "~3.0",
    "symfony/framework-bundle" : "~3.0",
    "symfony/monolog-bundle"   : "~2.11",
    "symfony/phpunit-bridge"   : "~3.0",
    "phpunit/phpunit"          : "~5.0",
    "codacy/coverage"          : "~1.0",
    "satooshi/php-coveralls"   : "~1.0"
  },
  "autoload" : {
    "psr-4" : {
      "SR\\Augustus\\InflectorBundle\\" : "src/"
    }
  },
  "autoload-dev" : {
    "psr-4" : {
      "SR\\Augustus\\InflectorBundle\\Tests\\" : "tests/"
    }
  },
  "minimum-stability" : "dev",
  "prefer-stable"     : true,
  "config" : {
    "bin-dir" : "bin"
  },
  "extra" : {
    "branch-alias" : {
      "dev-master" : "0.1-dev"
    }
  }
}
